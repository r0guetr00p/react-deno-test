{
  "tasks": {
    "dev": "deno run -A --node-modules-dir=auto npm:vite",
    "build": "deno run -A --node-modules-dir=auto npm:vite build",
    "server:start": "deno run -A --env-file --node-modules-dir --watch ./server/main.ts",
    "serve": "deno task server:start"
  },
  "imports": {
    "@/": "./client/src/",
    "#/": "./",
    "@deno/vite-plugin": "npm:@deno/vite-plugin@^1.0.0",
    "@hookform/resolvers": "npm:@hookform/resolvers@^5.0.1",
    "@oak/oak": "jsr:@oak/oak@^17.1.3",
    "@open-schemas/zod": "jsr:@open-schemas/zod@^2.0.0",
    "@radix-ui/react-avatar": "npm:@radix-ui/react-avatar@^1.1.3",
    "@radix-ui/react-dropdown-menu": "npm:@radix-ui/react-dropdown-menu@^2.1.12",
    "@radix-ui/react-label": "npm:@radix-ui/react-label@^2.1.4",
    "@radix-ui/react-slot": "npm:@radix-ui/react-slot@^1.2.0",
    "@std/assert": "jsr:@std/assert@1",
    "@supabase/supabase-js": "jsr:@supabase/supabase-js@^2.49.4",
    "@tailwindcss/vite": "npm:@tailwindcss/vite@^4.1.1",
    "@tanstack/react-table": "npm:@tanstack/react-table@^8.21.3",
    "@types/react": "npm:@types/react@^19.1.0",
    "@vitejs/plugin-react": "npm:@vitejs/plugin-react@^4.3.3",
    "class-variance-authority": "npm:class-variance-authority@^0.7.1",
    "clsx": "npm:clsx@^2.1.1",
    "lucide-react": "npm:lucide-react@^0.487.0",
    "react": "npm:react@^19.1.0",
    "react-dom": "npm:react-dom@^19.1.0",
    "react-hook-form": "npm:react-hook-form@^7.56.1",
    "react-router-dom": "npm:react-router-dom@^6.28.0",
    "shadcn-ui": "npm:shadcn-ui@^0.9.5",
    "tailwind-merge": "npm:tailwind-merge@^3.1.0",
    "tailwind-variants": "npm:tailwind-variants@^1.0.0",
    "tailwindcss": "npm:tailwindcss@^4.1.1",
    "tw-animate-css": "npm:tw-animate-css@^1.2.5",
    "vite": "npm:vite@^6.2.5"
  },
  "compilerOptions": {
    "types": [
      "react",
      "react-dom",
      "@types/react"
    ],
    "lib": [
      "dom",
      "dom.iterable",
      "deno.ns"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "nodeModulesDir": "auto"
}
